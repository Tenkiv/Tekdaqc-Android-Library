<manifest
    package="com.tenkiv.tekdaqc.android"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk android:minSdkVersion="5"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">

        <service
            android:name=".application.service.TekdaqcLocatorManager$LocatorService"
            android:exported="false"/>

        <service
            android:name=".application.service.TekdaqcCommunicationManager$ComService"
            android:exported="false"/>

        <receiver android:name=".application.service.TekdaqcLocatorManager$LocatorReceiver" >
            <intent-filter>
                <action android:name="com.tenkiv.tekdaqc.LOCATOR" />
            </intent-filter>
        </receiver>

    </application>

</manifest>
